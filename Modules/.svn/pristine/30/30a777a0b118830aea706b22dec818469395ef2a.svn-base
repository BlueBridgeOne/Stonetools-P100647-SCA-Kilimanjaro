define('ContactUsTest.Router'
, [
    'Backbone'
  , 'ContactUsTest.View'
  , 'ContactUsTest.Model'
  ]
, function
  (
    Backbone
  , ContactUsTestView
  , ContactUsTestModel
  )
{
  'use strict';

  return Backbone.Router.extend({
    routes:
    {
      'contact-us-test': 'contactUsTest'
    }

  , initialize: function(application)
    {
      this.application = application;
    }

  , contactUsTest: function(options)
    {
      var view = new ContactUsTestView({
        application: this.application,
        model: new ContactUsTestModel()
      });

      view.showContent();
    }
  });
});