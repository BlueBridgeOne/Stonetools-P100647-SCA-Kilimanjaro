{{!BB1 G Truslove 2017}} {{#if endoftype}}
<div class="col-xs-12">
	<div class="articles-more-container"><a class="articles-more" href="/{{typeurl}}">More</a></div>
</div>
{{/if}} {{#unless endoftype}} {{#if firsttype}}
<div class="clearfix"></div>
<div class="col-xs-12">
	<div class="articles-padding" style="border-top:2px solid {{colour}};">
		<h3 class="articles-type" style="color:{{colour}}">{{translate typetext}}</h3>
	</div>
</div>
{{/if}} {{#if largebanner}}
<div class="col-xs-12">
	<div class="articles-padding-banner">
		<a href="{{href}}" data-navigation="{{nav}}">
			<table class="articles-banner-table" style="background-color:{{colour}}">
				<tr>
					{{#if imageleft}}
					{{#if image}}
					<td class="articles-banner-image-td" style="background-image:url('{{image}}');">
					</td>
					{{/if}}
					{{/if}}
					<td class="articles-banner-td">
						<p class="articles-banner-type">{{translate typetext}}</p>
						<h2 class="articles-banner-title">{{name}}</h2>
					</td>
					{{#unless imageleft}}
					{{#if image}}
					<td class="articles-banner-image-td" style="background-image:url('{{image}}');">
					</td>
					{{/if}}
					{{/unless}}
				</tr>
			</table>
		</a>
	</div>
</div>
{{/if}} {{#if mediumimage}}
<div class="col-xs-6 col-sm-4 col-md-4 col-lg-3">
	<div class="articles-padding">
		<a href="{{href}}" data-navigation="{{nav}}">
			<div class="articles-image-box" style="background-color:{{colour}};background-image:url('{{image}}');"></div>
			<h5 class="articles-image-title">{{name}}</h5>
		</a>
	</div>
</div>
{{/if}} {{#if standard}} {{#if firstpriority}}
<div class="clearfix"></div>{{/if}}
<div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
	<div class="articles-padding articles-standard-break">
		<a href="{{href}}" data-navigation="{{nav}}">
			<h5 class="articles-standard-title"><span class="articles-standard-type" style="color:{{colour}}">{{translate typetext}}</span> - {{name}}</h5></a>
	</div>
</div>
{{/if}} {{#if archive}} {{#if firstpriority}}
<div class="clearfix"></div>{{/if}}
<div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
	<div class="articles-padding articles-standard-break">
		<a href="{{href}}" data-navigation="{{nav}}">
			<h5 class="articles-standard-title"><span class="articles-standard-type" style="color:{{colour}}">{{translate typetext}}</span> - {{name}}</h5></a>
	</div>
</div>
{{/if}} {{/unless}}