
<div class="sendtoollistemail-email" data-input="email" data-validation="control-group">
                <label for="email">{{translate 'Email'}}<small class="sendtoollistemail-required">*</small></label>
                <span data-validation="control">
                    <input name="sendtoollistemail-email" type="email" id="sendtoollistemail-email" maxlength="50" >
                </span>
            </div>
<button class="sendtoollistemail-send-button" data-action="confirm" type="submit">{{translate 'Send'}}</button>
            <button class="sendtoollistemail-cancel-button" data-action="cancel">{{translate 'Cancel'}}</button>
            <div class="clearfix"></div>
            