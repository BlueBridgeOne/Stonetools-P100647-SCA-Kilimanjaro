define('Articles.Home.View', [
	'Backbone', 'articles_home.tpl', 'Backbone.CollectionView', 'Backbone.CompositeView', 'Articles.Details.View', 'Article'
], function(Backbone, articles_home_tpl, CollectionView, CompositeView, ArticlesDetailsView, Article) {
	return Backbone.View.extend({
		initialize: function(options) {
			CompositeView.add(this);
			this.application = options.application;
			this.collection = options.collection;
			this.type = options.type;
		},
		childViews: {
			'Articles.Collection': function() {
				return new CollectionView({
					'childView': ArticlesDetailsView,
					'collection': this.collection,
					'type': this.type
				});
			}
		},
		template: articles_home_tpl,
		getContext: function() {
			return { }
		}
	});
});