.contact-management-detail{
	@extend .box-column-bordered
}

.contact-management-detail-button-back{
    @extend .button-back;
}

.contact-management-detail-button-back-icon{
	@extend .button-back-icon;
}

.contact-management-detail-required {
	display:block;
	margin-top: 10px;
	margin-bottom: 20px;
}

.contact-management-detail-controls-group{
	@extend .control-group;
}

.contact-management-detail-label{
	/*@extend .input-label;*/
}

.contact-management-detail-required-star {
	@extend .input-required;
}

.contact-management-detail-input, .contact-management-detail-select{
	@extend .input-large;
}

.contact-management-detail-textarea{
	/*@extend .input-textarea;*/
	display: block;
	margin-bottom: 10px;
	width: 100%;
}

.contact-management-detail-button-submit{
	@extend .button-medium;
	@extend .button-primary;
}

.contact-management-detail-button {
	@extend .button-medium;
}

