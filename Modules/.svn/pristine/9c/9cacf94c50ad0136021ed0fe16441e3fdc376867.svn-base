.footer-simplified-content{
border-top:2px solid darken($st-color-lightgrey,30);
    @extend .container;
    @extend .row;
    margin: auto;
}

.footer-simplified-content-nav {
    @extend .col-md-8;
    @extend .col-xs-12;
    padding-left: 0;
    padding-right: 0;
}

.footer-subtext {
    font-size: .9em;
    line-height: 1.3;
    color: $st-color-footer-link;
    padding-top:$sc-padding-lv3;
    padding-bottom:$sc-padding-lv3;
}