$sc-main-header: 242px;
$sc-main-mobile-header: 133px;

.shopping-layout {
	@extend %sc-layout;
	padding-top:$sc-main-mobile-header;

	@media (min-width: $screen-md-min) {
	padding-top:$sc-main-header;
	}
}

.shopping-layout-header {
	margin-bottom: $sc-margin-lv2;
position:fixed;
top:0;
z-index:1000;
  	@media (min-width: $screen-sm-min){
		margin-bottom: $sc-margin-lv3;
		

	}
}

.shopping-layout-footer {
	@extend %sc-footer !optional;
}

.shopping-layout-breadcrumb {
	@extend .container;
}
.shopping-layout-breadcrumb-container{

margin-bottom:$sc-margin-lv1;

@media (min-width: $screen-md-min){
border-bottom: 1px solid $sc-color-theme-border;
		margin-bottom:$sc-margin-lv4;
	}
}

.shopping-layout-no-javascript-msg {
	@extend .message-warning;
	width: 100%;
}

.shopping-layout-content{}
